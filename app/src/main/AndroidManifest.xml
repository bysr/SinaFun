<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="hipad.unittest">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity
            android:name=".RecordVideoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".VideoPreviewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="hipad.animal.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="hipad.animal.SecondActivity">
        </activity>
        <activity android:name="hipad.animal.LineChartActivity2">
        </activity>
        <activity android:name="hipad.animal.LineChartActivityColored">
        </activity>
        <activity android:name="hipad.animal.LineChartTime">
        </activity>
        <activity android:name="hipad.animal.SinaActivity">
        </activity>
        <activity android:name="hipad.animal.SinaActivity2">
        </activity>
        <activity android:name="hipad.animal.CubicLineChartActivity"/>
        <activity android:name="hipad.dialog.DialogActivity">
        </activity>
    </application>

</manifest>